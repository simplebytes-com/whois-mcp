  },
  "homepage": "https://github.com/simplebytes-com/whois-mcp#readme",
  "bugs": {
    "url": "https://github.com/simplebytes-com/whois-mcp/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "node-fetch": "^3.3.2"
